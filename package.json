        "jwt-decode": "3.1.2",
        "lottie-web": "5.9.2",
        "moment": "2.29.2",
        "react": "18.0.0",
        "react-cookie": "4.1.1",
        "react-dom": "18.0.0",
        "react-hook-form": "7.29.0",
        "react-i18next": "11.16.6",
        "react-native-fast-image": "8.5.11",
        "react-native-svg-web": "1.0.9",
        "react-native-web": "0.17.7",
        "react-native-web-linear-gradient": "1.1.2",
        "react-native-web-lottie": "1.4.4",
        "react-router-dom": "6.3.0",
        "sass": "1.50.0",
        "styled-components": "5.3.5",
        "typescript": "4.6.3",
        "uuid": "8.3.2",
        "victory": "36.3.1",
        "vite": "2.9.2",
        "vite-plugin-svgr": "1.1.0",
        "web-vitals": "2.1.4",
        "yup": "0.32.11"
    },
    "nohoist": [
        "**/react-native",
        "**/react-native/**"
    ],
    "scripts": {
        "start": "env-cmd -f ./environments/.dev.env vite",
        "preview": "vite preview",
        "build": "tsc && vite build",
        "unit": "tsc && jest --coverage",
        "format:fix": "prettier --write \"{src,__tests__,.storybook}/**/*.{ts,tsx,scss,js}\" \"**/*.yml\" \"**/*.json\" \"README.md\" ",
        "format:check": "prettier --check \"src/**/*.{ts,tsx,scss}\" ",
        "lint": "eslint --ext .ts,.tsx,.json .",
        "lint:fix": "eslint --ext .ts,.tsx,.json . --fix",
        "audit": "better-npm-audit audit",
        "test": "npm run format:check && npm run lint",
        "storybook": "start-storybook -p 6006",
        "import-content": "node scripts/import-content.js",
        "build-storybook": "build-storybook"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "engines": {
        "node": ">=14.x <=16.x"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "2.6.2",
        "@graphql-codegen/typescript": "2.4.8",
        "@graphql-codegen/typescript-operations": "2.3.5",
        "@graphql-codegen/typescript-react-apollo": "3.2.11",
        "@storybook/addon-actions": "6.5.0-alpha.54",
        "@storybook/addon-essentials": "6.5.0-alpha.54",
        "@storybook/addon-links": "6.5.0-alpha.54",
        "@storybook/builder-vite": "0.1.28",
        "@storybook/node-logger": "6.5.0-alpha.54",
        "@storybook/react": "6.5.0-alpha.54",
        "@swc/core": "1.2.165",
        "@swc/jest": "0.2.20",
        "@testing-library/jest-dom": "5.16.4",
        "@types/js-cookie": "3.0.1",
        "@types/moment-timezone": "0.5.30",
        "@types/node": "17.0.23",
        "@types/react-test-renderer": "18.0.0",
        "@types/uuid": "8.3.4",
        "@typescript-eslint/eslint-plugin": "5.19.0",
        "@typescript-eslint/parser": "5.19.0",
        "babel-jest": "27.5.1",
        "better-npm-audit": "3.7.3",
        "eslint": "8.13.0",
        "jest": "27.5.1",
        "jest-canvas-mock": "2.3.1",
        "moment-timezone": "0.5.34",
        "nock": "13.2.4",
        "postcss-normalize": "10.0.1",
        "postcss-preset-env": "7.4.3",
        "prettier": "2.6.2",
        "react-test-renderer": "18.0.0"
    }
}
